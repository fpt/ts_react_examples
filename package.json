{
  "name": "rplayground",
  "version": "1.0.0",
  "main": "app.tsx",
  "author": "Yoichi Fujimoto",
  "license": "MIT",
  "scripts": {
    "build": "NODE_ENV=production webpack",
    "build-storybook": "build-storybook",
    "jest": "jest",
    "lint": "npm-run-all lint:*",
    "lint:ts": "tslint './src/**/*.ts{,x}'",
    "start": "NODE_ENV=development webpack-dev-server --port 3000",
    "storybook": "start-storybook -p 6006",
    "test": "npm-run-all lint jest"
  },
  "dependencies": {
    "@types/react": "^16.0.10",
    "@types/react-dom": "^16.0.1",
    "@types/react-redux": "^5.0.14",
    "@types/recompose": "^0.24.3",
    "@types/webpack": "^3.8.1",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-redux": "^5.0.6",
    "recompose": "^0.26.0",
    "redux": "^3.7.2",
    "rxjs": "^5.5.6",
    "typescript": "^2.6.2",
    "webpack": "^3.10.0",
    "webpack-blocks": "^1.0.0-rc.2",
    "webpack-blocks-utils": "^2.0.0-alpha.4"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^3.2.17",
    "@storybook/cli": "^3.2.17",
    "@storybook/react": "^3.2.17",
    "@types/html-webpack-plugin": "^2.30.1",
    "@types/jest": "^21.1.8",
    "@types/redux-mock-store": "^0.0.12",
    "@types/redux-thunk": "^2.1.0",
    "@types/storybook__addon-actions": "^3.0.1",
    "@types/storybook__addon-knobs": "^3.2.1",
    "@types/storybook__react": "^3.0.5",
    "@types/stylelint": "^7.10.0",
    "@types/webfontloader": "^1.6.29",
    "@types/webpack-dev-server": "^2.9.2",
    "@webpack-blocks/dev-server2": "^0.4.0",
    "@webpack-blocks/typescript": "^1.0.0-rc",
    "@webpack-blocks/webpack2": "^0.4.0",
    "html-webpack-plugin": "^2.30.1",
    "jest": "^21.2.1",
    "npm-run-all": "^4.1.2",
    "raf": "^3.4.0",
    "ts-jest": "^21.2.4",
    "ts-node": "^3.3.0",
    "tslint": "^5.8.0",
    "tslint-config-airbnb": "^5.4.2",
    "tslint-react": "^3.2.0",
    "tsutils": "^2.13.0",
    "typescript-styled-plugin": "^0.2.2",
    "webpack-dev-server": "^2.9.5"
  },
  "engines": {
    "node": "8.x",
    "yarn": "1.x"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testMatch": [
      "**/*.spec.(ts|tsx)"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx"
    ],
    "modulePaths": [
      "src"
    ]
  }
}
